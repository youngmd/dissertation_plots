p <- ggplot(Dstats, aes(x=pred, y=DIC, fill=pred)) +
geom_bar(stat="identity") +
geom_text(aes(x=pred,
y=DIC + 0.3 * sign(DIC),
label=format(Dstats$DIC, digits=4)),
hjust=0,
size=3,
color=rgb(100,100,100, maxColorValue=255)) +
labs(x="", y="", title=as.expression( bquote( "DIC"~"of"~N[GC]~"Predictors  N ="~.(nrow(GCS)) ))) +
theme(axis.text.x =
element_text(size  = 10,
angle = 0,
hjust = 1,
vjust = 1),
axis.text.y =
element_text(hjust = 0.5),
panel.background = element_blank(),
panel.grid.minor = element_blank(),
axis.ticks  = element_blank(),
axis.line   = element_line(colour=NA),
axis.line.x = element_line(colour="grey80")) +
theme(legend.position="none") +
coord_flip() +
scale_x_discrete(labels=parse(text=c("log~M[dyn]/M['\u0298']","log~M['*']/M['\u0298']","R[eff]","L[K]","L[V]")))
p
